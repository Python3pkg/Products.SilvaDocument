<init tal:omit-tag=""
  tal:define="global disable_delete python:1;
    global disable_move_up python:1;
    global disable_move_down python:1;
    global type here/get_list_type;
    global insert_now python:1" />
<div metal:use-macro="container/macro_embed_view/macros/embed_view">
<div metal:fill-slot="embedded_elements" tal:omit-tag="">
  <content tal:replace="structure here/content" />
</div>

<div metal:fill-slot="element_controls" tal:omit-tag="">
<table border="0" width="100%" cellspacing="0">
<tr>
<td>
<enable tal:omit-tag="" tal:replace="nothing">
  &nbsp;Move item(s) to position: 
  <select class="manipulator" name="move_to">
    <option>(not yet)</option>
  </select>
  &nbsp;
  <input class="button" type="submit" value=" Move " 
    notal:attributes="name string:${rel_url_start}/save:method" />
&nbsp;&nbsp;&nbsp;
</enable>
</td>
<td class="align-right">
Type: 
<select class="manipulator"
  style="text-transform:none;"
  name="element_type">
<option value="disc"
  tal:attributes="selected python:test(type == 'disc', 'selected', nothing)">disc bullets</option>
<option value="square"
  tal:attributes="selected python:test(type == 'square', 'selected', nothing)">square bullets</option>
<option value="circle"
  tal:attributes="selected python:test(type == 'circle', 'selected', nothing)">circle bullets</option>
<option value="1"
  tal:attributes="selected python:test(type == '1', 'selected', nothing)">1, 2, 3,..</option>
<option value="I"
  tal:attributes="selected python:test(type == 'I', 'selected', nothing)">I, II, III,..</option>
<option value="i"
  tal:attributes="selected python:test(type == 'i', 'selected', nothing)">i, ii, iii,..</option>
<option value="A"
  tal:attributes="selected python:test(type == 'A', 'selected', nothing)">A, B, C,..</option>
<option value="a"
  tal:attributes="selected python:test(type == 'a', 'selected', nothing)">a, b, c,..</option>
<option value="none"
  tal:attributes="selected python:test(type == 'none', 'selected', nothing)">no bullet</option>
</select>
</td>
</tr>
</table>
</div>
</div>
